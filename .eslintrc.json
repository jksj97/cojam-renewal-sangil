{
  "extends": ["eslint:recommended", "plugin:react/recommended", "plugin:prettier/recommended"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2021
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },   
  "rules": {
    "no-undef": "off",
    "no-unused-vars": "off",
    "react/prop-types": "off"

  }
}